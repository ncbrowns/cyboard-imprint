/ {
    chosen { zmk,matrix-transform = &imprint_number_row; };

    behaviors {
        hrm_left: hrm_left {
            compatible = "zmk,behavior-hold-tap";
            label = "HRM_LEFT";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <250>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&kp>, <&kp>;

            hold-trigger-key-positions = <6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 42 43 44 45 46 47 50 51 55 56 57 61 62 63>;
        };

        hrm_left_index: hrm_left_index {
            compatible = "zmk,behavior-hold-tap";
            label = "HRM_LEFT_INDEX";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <150>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&kp>, <&kp>;

            hold-trigger-key-positions = <55 56 61 62 6 7 8 9 10 11 18 19 20 21 22 23 30 31 32 33 34 35 42 43 44 45 46 47 50 51 55 56 57 61 62 63>;
        };

        hrm_right: hrm_right {
            compatible = "zmk,behavior-hold-tap";
            label = "HRM_RIGHT";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <250>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&kp>, <&kp>;

            hold-trigger-key-positions = <0 1 2 3 4 5 12 13 14 15 16 17 24 25 26 27 28 29 36 37 38 39 40 41 48 49 52 53 54 58 59 60>;
        };

        hrm_right_index: hrm_right_index {
            compatible = "zmk,behavior-hold-tap";
            label = "HRM_RIGHT_INDEX";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <150>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&kp>, <&kp>;

            hold-trigger-key-positions = <0 1 2 3 4 5 12 13 14 15 16 17 24 25 26 27 28 29 36 37 38 39 40 41 48 49 52 53 54 58 59 60>;
        };

        thumb_layer: thumb_layer {
            compatible = "zmk,behavior-hold-tap";
            label = "THUMB_LAYER";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <250>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&mo>, <&kp>;
        };

        thumb_tap_hold: thumb_tap_hold {
            compatible = "zmk,behavior-hold-tap";
            label = "THUMB_TAP_HOLD";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            quick-tap-ms = <300>;
            require-prior-idle-ms = <250>;
            flavor = "balanced";
            hold-trigger-on-release;
            bindings = <&kp>, <&kp>;
        };

        cwminus: cwminus {
            compatible = "zmk,behavior-hold-tap";
            label = "CWMINUS";
            bindings = <&cwexit>, <&kp>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "tap-preferred";
        };

        shift_caps: shift_caps {
            compatible = "zmk,behavior-tap-dance";
            label = "SHIFT_CAPS";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp LSHIFT>, <&to 7>;
        };
    };

    macros {
        thanks_pat: thanks_pat {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_tap_time 2>,
                <&kp LS(T) &kp H &kp A &kp N &kp K &kp S &kp COMMA &kp LS(RET) &kp LS(P) &kp A &kp T>;

            label = "THANKS_PAT";
        };

        triple_bquote: triple_bquote {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp GRAVE &kp GRAVE &kp GRAVE>;
            label = "TRIPLE_BQUOTE";
        };

        browser_url: browser_url {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(L) &kp LC(C) &kp ESC &kp ESC>;
            label = "BROWSER_URL";
        };

        ctrl_shift_tab: ctrl_shift_tab {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_wait_time 2>,
                <&macro_press>,
                <&kp LCTRL &kp LSHFT>,
                <&macro_tap>,
                <&kp TAB>,
                <&macro_release>,
                <&kp LCTRL &kp LSHFT>;

            label = "CTRL_SHIFT_TAB";
        };

        tab_switcher: tab_switcher {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&tog 6>,
                <&macro_press>,
                <&kp LALT>,
                <&macro_tap>,
                <&kp TAB>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&kp LALT>,
                <&macro_tap>,
                <&tog 6>;

            label = "TAB_SWITCHER";
        };

        cwshift: cwshift {
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings =
                <&macro_press>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&macro_param_1to1 &kp MACRO_PLACEHOLDER>,
                <&macro_release>,
                <&kp LSHFT>;

            label = "CWSHIFT";
        };

        cwexit: cwexit {
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings = <&tog 7 &macro_param_1to1 &kp MACRO_PLACEHOLDER>;
            label = "CWEXIT";
        };

        cwexitshift: cwexitshift {
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings =
                <&tog 7>,
                <&macro_press>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&macro_param_1to1 &kp MACRO_PLACEHOLDER>,
                <&macro_release>,
                <&kp LSHFT>;

            label = "CWEXITSHIFT";
        };

        cwmagic: cwmagic {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&tog 7 &mo 4>;
            label = "CWMAGIC";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        Home {
            bindings = <
&kp EQUAL    &kp N1            &kp N2            &kp N3             &kp N4                     &kp N5                          &kp N6    &kp N7                    &kp N8                     &kp N9             &kp N0                &kp MINUS
&kp TAB      &kp Q             &kp W             &kp E              &kp R                      &kp T                           &kp Y     &kp U                     &kp I                      &kp O              &kp P                 &kp BSLH
&kp ESC      &hrm_left LWIN A  &hrm_left LALT S  &hrm_left LCTRL D  &hrm_left_index LSHFT F    &kp G                           &kp H     &hrm_right_index RSHFT J  &hrm_right RCTRL K         &hrm_right RALT L  &hrm_right RWIN SEMI  &kp SQT
&shift_caps  &kp Z             &kp X             &kp C              &kp V                      &kp B                           &kp N     &kp M                     &kp COMMA                  &kp DOT            &kp FSLH              &shift_caps
                               &kp LEFT          &kp RIGHT                                                                                                         &kp UP                     &kp DOWN
                                                                    &thumb_tap_hold LCTRL ESC  &kp LALT            &kp LWIN    &kp RWIN  &kp RALT                  &thumb_tap_hold RCTRL TAB
                                                                    &thumb_layer 1 BACKSPACE   &thumb_layer 2 DEL  &mo 4       &mo 4     &thumb_layer 3 ENTER      &kp SPACE
            >;
        };

        Navigation {
            bindings = <
&none      &kp LA(LC(F1))  &kp LA(LC(F2))  &kp LA(LC(F3))   &none        &none                   &kp TILDE       &kp INS           &none             &none              &none           &none
&none      &none           &kp LC(W)       &ctrl_shift_tab  &kp LC(TAB)  &kp F5                  &kp HOME        &kp PAGE_DOWN     &kp PG_UP         &kp END            &thanks_pat     &kp LC(LA(K))
&none      &kp LGUI        &kp LALT        &kp LCTRL        &kp LSHFT    &tab_switcher           &kp LEFT        &kp DOWN          &kp UP            &kp RIGHT          &kp GRAVE       &kp LC(PAUSE_BREAK)
&kp LSHFT  &kp LC(Z)       &kp LC(X)       &kp LC(C)        &kp LC(V)    &browser_url            &kp LEFT_BRACE  &kp RIGHT_BRACE   &kp LEFT_BRACKET  &kp RIGHT_BRACKET  &triple_bquote  &kp RSHFT
                           &kp LEFT        &kp RIGHT                                                                               &kp UP            &kp DOWN
                                                            &none        &none          &none    &kp C_VOL_DN    &kp C_MUTE        &kp C_VOL_UP
                                                            &none        &none          &none    &kp C_PREV      &kp C_PLAY_PAUSE  &kp C_NEXT
            >;
